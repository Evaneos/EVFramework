{
    "name": "evaneos/evframework",
    "description": "Evaneos Framework stack with glue",
    "version": "1.0.0",
    "type": "application",
    "homepage": "http://github.com/Evaneos/Framework",
    "time": "2014-07-02",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Evaneos",
            "email": "admin@evaneos.com",
            "homepage": "http://www.evaneos.com",
            "role": "Admin"
        }
    ],
    "autoload": {
        "psr-4": {
            "EVFramework\\": "src/"
        },
        "files" : [
            "src/Translation/t.function.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:oliviermadre/dic-it.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:oliviermadre/translations.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Evaneos/pyrite.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Evaneos/trolamine.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:Evaneos/berthe.git"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    },
    "require": {
        "oliviermadre/translations": "dev-master",
        "oliviermadre/dic-it": "dev-master",
        "evaneos/pyrite": "dev-master",
        "evaneos/trolamine": "dev-master",
        "evaneos/berthe" : "dev-master-namespace",
        "monolog/monolog": "1.7.0",
        "league/fractal": "~0.7.0",
        "twig/twig": "1.*",
        "asm89/stack-cors": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "squizlabs/php_codesniffer": "1.5.*"
    }
}
